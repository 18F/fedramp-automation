         <risk id="risk-3-1">
            <title>Vulnerability Title</title>
            <description>
               <p>This is a description of the vulnerability provided by the tool.</p>
            </description>
            
            <prop name="vulnerability-id" class="scanner-name">VulID-001</prop>
